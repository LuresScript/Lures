local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/tbao143/thaibao/main/Librarykavoui"))()
local window = lib.CreateLib("Key | Lures Script", "DarkTheme")

local Main = window:NewTab("Pegar Key")
local Section = Main:NewSection("Key System")

_G.Hwid = game:GetService("RbxAnalyticsService"):GetClientId()
_G.Key = "nothing yet"

Section:NewButton("Copiar link", "Paste link in browser", function()
    setclipboard("https://gateway.platoboost.com/a/89205?id="..game:GetService("RbxAnalyticsService"):GetClientId())) -- Add your key link
end)

local KeyLabel = Section:NewLabel("Key 1 | ".. _G.Key)
local KeyInput = Section:NewTextBox("Key", "Key",
    function(txt)
        KeyLabel:UpdateLabel("Key > " .. txt)
        _G.Key = txt
        CheckKey()
    end)

_G.CheckKey = false
local OutputLabel = Section:NewLabel("")  
function CheckKey()
    if _G.CheckedKey == true then
        return
    end
    local key = game:HttpGet("https://api-gateway.platoboost.com/v1/public/whitelist/89205/89205?key=%s")
    if key == 'False' then
        OutputLabel:UpdateLabel("Key incorrect")
        return
    end


    _G.CheckedKey = true
    loadstring(game:HttpGet("https://raw.githubusercontent.com/LuresScript/Lures/refs/heads/main/dark"))()
end
