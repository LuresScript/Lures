    M.Main:AddButton({
        Title = "Resgatar",
        Description = "Aperte para resgatar",
        Callback = function()
            function RedeemCode(value)
                game:GetService("ReplicatedStorage").Remotes.Redeem:InvokeServer(value)
            end
            for i,v in pairs(x2Code) do
                RedeemCode(v)
            end
        end})

        M.Main:AddParagraph({
            Title = "Farm Level",
            Content = ""
        })

        local Toggle = M.Main:AddToggle("MyToggle", {Title = "Farm Level", Default = false })

        Toggle:OnChanged(function()
            _G.AutoFarm = Value
        StopTween(_G.AutoFarm)
        end)

        M.Main:AddParagraph({
            Title = "Farm Maestria",
