local function TIBURLS_fake_script() -- Mini.MiniS 
	local script = Instance.new('LocalScript', Mini)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(TIBURLS_fake_script)()
local function DIRFNM_fake_script() -- Maximise.MaxS 
	local script = Instance.new('LocalScript', Maximise)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Backgrounds.Visible = true
	end)
end
coroutine.wrap(DIRFNM_fake_script)()

