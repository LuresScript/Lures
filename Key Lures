local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Sistema de Key", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroText = "Lures Key..."})

OrionLib:MakeNotification({
	Name = "Notificação",
	Content = "Logado na conta "..Player.Name..".",
	Image = "rbxassetid://4483345998",
	Time = 5
})

_G.Key = "KEY_aa4j0635"
_G.KeyInput = "string"

function MakeScriptHub()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/LuresScript/Lures/refs/heads/main/README.md"))()
end

function CorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Key Correta",
        Content = "Você colocou a Key certa!",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

function IncorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Key Incorreta",
        Content = "Você colocou a Key errada",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

local Tab = Window:MakeTab({
	Name = "Creditos",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Ta = Window:MakeTab({
	Name = "Pegar Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Redes Sociais"
})

Tab:AddButton({
	Name = "Discord",
	Callback = function()
        https://discord.com/invite/qE5CM9Sp3A
  	end    
})

Tab:AddButton({
	Name = "YouTube",
	Callback = function()
        https://www.youtube.com/@eolures
  	end    
})

Tab:AddButton({
	Name = "TikTok",
	Callback = function()
      	print("Sem Link")
  	end    
})

Ta:AddTextbox({
	Name = "Cole a Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

Ta:AddButton({
	Name = "Verificar Key",
	Callback = function()
        if _G.KeyInput == _G.Key then
            MakeScriptHub()
            CorrectKeyNotification()
        else
            IncorrectKeyNotification()
        end
  	end    
})

Ta:AddButton({
	Name = "Pegar Key",
	Callback = function()
        setclipboard("https://loot-link.com/s?08fde99c")
  	end    
})
