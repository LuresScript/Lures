local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Sistema de Key", HidePremium = false, SaveConfig = true, ConfigFolder = "KeyTest", IntroText = "Loading..."})

_G.Key = ""
_G.CheckKey = false

local Tab = Window:MakeTab({
	Name = "Pegar Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Pegar Key"
})

Tab:AddTextbox({
	Name = "Colocar a Key",
	Default = "Cole a Key",
	TextDisappear = true,
	Callback = function(Value)
		_G.Key = Value
	end	  
})

Tab:AddButton({
	Name = "Pegar Key",
	Callback = function()
      	setclipboard("https://gateway.platoboost.com/a/83268?id=123456")
  	end    
})

local key = game:HttpGet("https://gateway.platoboost.com/a/83268?id=123456")

Tab:AddButton({
	Name = "Verificar Key",
	Callback = function()
	    if key == false then
            print("Key Errada")
        else
            loadstring(game:HttpGet("https://raw.githubusercontent.com/LuresScript/Lures/refs/heads/main/Script"))()
        end
    end
})
