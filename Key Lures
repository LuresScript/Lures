_G.Key = "KEY_aa4j0635"
_G.KeyInput = "string"

function MakeScriptHub()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/LuresScript/Lures/refs/heads/main/README.md"))()
end

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Sistema de Key", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroText = "Loading Lures Key..."})

OrionLib:MakeNotification({
	Name = "Logado em",
	Content = "Logado na conta "..Player.Name..".",
	Image = "",
	Time = 5
})

function CorrectKeyNotification()
	OrionLib:MakeNotification({
	Name = "Key Correta",
	Content = "Você colocou a Key Certa",
	Image = "",
	Time = 5
})
end

function IncorrectCorrectKeyNotification()
	OrionLib:MakeNotification({
	Name = "Key Errada",
	Content = "Você colocou a Key Errada",
	Image = "",
	Time = 5
})
end

local C = Window:MakeTab({
	Name = "Creditos",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

C:AddLabel("É o Lures")

C:AddLabel("Entre no Discord")

C:AddButton({
	Name = "Discord",
	Callback = function()
      		setclipboard("https://discord.com/invite/qE5CM9Sp3A")
  	end    
})

C:AddLabel("Se inscreva no YouTube")

C:AddButton({
	Name = "YouTube",
	Callback = function()
      		setclipboard("https://www.youtube.com/@eolures")
  	end    
})

local K = Window:MakeTab({
	Name = "Pegar Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

K:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

K:AddButton({
	Name = "Verificar Key",
	Callback = function()
      		if _G.KeyInput == _G.Key then
		MakeScriptHub()
		CorrectKeyNotification()
		else
			IncorrectCorrectKeyNotification()
		end
  	end    
})

Tab:AddButton({
	Name = "Copiar Link",
	Callback = function()
      		setclipboard("https://loot-link.com/s?9466c87c")
  	end    
})
