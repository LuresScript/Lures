_G.Key = "1234"

local keychecked = false
local hwid = nil
local hwidplr = game:GetService("RbxAnalyticsService"):GetClientId()  -- Client ID, não HWID

if _G.Key == "1234" then
    hwid = "5ebabbf3-cd18-44da-903d-b8212ba15737"  -- HWID fixo (não real)
    keychecked = true
end

if keychecked then
    if hwidplr == hwid then
        -- Se a verificação for bem-sucedida (lembrando que hwidplr provavelmente não vai ser igual ao hwid)
        print("HWID check passed!")
        -- Alternativa segura para carregar o script (se necessário)
        local success, result = pcall(function()
            return loadstring(game:HttpGet("https://raw.githubusercontent.com/pedrinn007x5/Lures/refs/heads/main/README.md"))()
        end)
        if success then
            print("Script loaded successfully!")
        else
            print("Failed to load script:", result)
        end
    else
        print("Incorrect hwid")
    end
else
    print("Incorrect key")
end
