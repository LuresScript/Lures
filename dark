_G.Key = "1234"

local keychecked = false
local hwid = "5ebabbf3-cd18-44da-903d-b8212ba15737"
local hwidplr = game:GetService("RbxAnalyticsService"):GetClientId()

if _G.Key == "1234" then
    keychecked = true
end

if keychecked then
    if hwidplr == hwid then
        print("HWID check passed!")
        local success, result = pcall(function()
            return loadstring(game:HttpGet("https://raw.githubusercontent.com/pedrinn007x5/Lures/refs/heads/main/README.md"))()
        end)
        if success then
            print("Script loaded successfully!")
        else
            print("Failed to load script:", result)
        end
    else
        print("Incorrect Hwid")
    end
else
    print("Incorrect Key")
end
