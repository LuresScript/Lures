_G.Key = "1234"

local keychecked = false
local hwid = nil
local hwidplr = game:GetService("RbxAnalyticsService"):GetClientId()

if _G.Key == "1234" then
    hwid = "5ebabbf3-cd18-44da-903d-b8212ba15737"
    keychecked = true
end

if keychecked then
    if hwidplr == hwid then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/pedrinn007x5/Lures/refs/heads/main/README.md"))()
    else
        print("Incorrect Hwid")
    end
else
    print("Incorrect Key")
end
